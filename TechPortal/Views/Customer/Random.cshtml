
@model TechPortal.ViewModels.ArbitraryCustomerViewModel
@{
    ViewBag.Title = "random";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.customer.CompanyName</h2>

<h1>Random List Of Products:</h1>
@foreach (var product in Model.products)
{
    <li>Item @product.ID | @product.Name.ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString().ToString()</li>
}


