@model TechPortal.ViewModels.ArbitraryCustomerViewModel
@{
    ViewBag.Title = "random";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.customer.CompanyName</h2>
<h3>Products Ordered</h3>
<ul>
    @foreach (var product in Model.products)
    {
        <li>@product.Name</li>
    }
</ul>
